{
    "env": {
        "browser": true,
        "mocha": true
    },
    "extends": "eslint-config-holy-grail",
    "globals": {
        "expect": false,
        "leche": false
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions": 0
    }
}
